xkb_symbols "remote" {
	key <LALT> {
		type= "ALPHABETIC",
		symbols[Group1]= [ g, G ]
	};
	key <LFSH> { [ Alt_L, Meta_L ] };
	key <CAPS> { [ Shift_L ] };
	key <AB01> { [ Control_L ] };
	modifier_map Control { <AB01> };
	key <AE04> {
		type= "ALPHABETIC",
		symbols[Group1] = [ l, L ]
	};
	key <AE03> {
		type= "ALPHABETIC",
		symbols[Group1]= [ n, N ]
	};
};
