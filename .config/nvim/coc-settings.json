{
	"python.pythonPath": "/home/rizhiy/.bin/coc_python.sh",
	"python.formatting.provider": "black",
	"python.formatting.blackArgs": [
		"--line-length",
		"120"
	],
	"coc.preferences.formatOnSaveFiletypes": [
		"python",
		"markdown",
		"json",
		"jsonc"
	],
	"explorer.width": 30,
	"explorer.contentWidth": 90,
	"explorer.icon.enableNerdfont": true,
	"explorer.previewAction.onHover": false,
	"explorer.file.child.template": " [indent][icon | 1] [git & 1][selection & 1 | clip & 1][diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5][size]",
	"explorer.keyMappings.global": {
		"<cr>": [
			"expandable?",
			[
				"expanded?",
				"collapse",
				"expand"
			],
			"open"
		],
		"v": "open:vsplit"
	},
	"explorer.file.hiddenRules": {
		"extensions": [
			"pyc",
			"egg-info"
		],
		"filenames": [
			"__pycache__"
		]
	}
}
