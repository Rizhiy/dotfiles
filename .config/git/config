[alias]
	lg = log --all --graph --oneline --abbrev-commit
	s = status
	c = commit
	d = diff
	a = add
	cm = commit -m
[push]
	default = simple
[core]
	editor = vim
	autocrlf = input
	excludesfile = /home/rizhiy/.config/git/ignore
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge --skip -- %f
	process = git-lfs filter-process --skip
	required = true
[color "diff"]
	meta = bold
	new = cyan
[submodule]
	recurse = true
[user]
	name = Artem Vasenin
